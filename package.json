{
  "devDependencies": {
    "@types/node": "^18.15.11",
    "axios": "^1.3.5",
    "eslint": "^8.53.0",
    "json-diff": "^1.0.6"
  },
  "scripts": {
    "build-banners": "node bin/build_banners.mjs"
  },
  "eslintConfig": {
    "root": true,
    "ignorePatterns": "bin/**",
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "browser": true,
      "es2017": true
    },
    "rules": {
      "no-extra-semi": "off",
      "no-unused-vars": [
        "warn",
        {
          "args": "all",
          "argsIgnorePattern": "^_",
          "caughtErrors": "all",
          "caughtErrorsIgnorePattern": "^_",
          "destructuredArrayIgnorePattern": "^_"
        }
      ]
    }
  }
}
